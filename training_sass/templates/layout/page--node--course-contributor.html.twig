<div id="page-wrapper">
  <div id="page">
    <header id="header" class="header" role="banner" aria-label="{{ 'Site header'|t}}">
      <div class="search-box collapse" id="collapseSearch">
        <form role="search">
          <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
          <button class="btn" type="submit">Search</button>
        </form>
        <a class="search-close collapsed" data-bs-toggle="collapse" href="#collapseSearch" role="button" aria-expanded="false" aria-controls="collapseSearch" tabindex="-1"><i class="fa-regular fa-x"></i></a>
      </div>
      {% block head %}
        {{ page.primary_menu }}
      {% endblock %}
    </header>
  <a id="button"></a>
    {% if page.highlighted %}
      <div class="highlighted">
        <aside class="{{ container }} section clearfix" role="complementary">
          {{ page.highlighted }}
        </aside>
      </div>
    {% endif %}
    {% if page.featured_top %}
      {% block featured %}
        <div class="featured-top">
          <aside class="featured-top__inner section {{ container }} clearfix" role="complementary">
            {{ page.featured_top }}
          </aside>
        </div>
      {% endblock %}
    {% endif %}
    <div id="main-wrapper" class="layout-main-wrapper clearfix">
      {% block content %}
        <div id="main" class="{{ container }}">
          <div class="row row-offcanvas row-offcanvas-left clearfix">
            <main{{ content_attributes }}>
              <section class="section">
                <a id="main-content" tabindex="-1"></a>
                {{ drupal_block('system_breadcrumb_block') }}
                {{ drupal_block('local_tasks_block') }}
                {{ drupal_view('contributors', 'page') }}
              </section>
            </main>
          </div>
        </div>
      {% endblock %}
    </div>
    {% if page.featured_bottom_first or page.featured_bottom_second or page.featured_bottom_third %}
      <div class="featured-bottom">
        <aside class="clearfix" role="complementary">
          {{ page.featured_bottom_first }}
          {{ page.featured_bottom_second }}
          {{ page.featured_bottom_third }}
        </aside>
      </div>
    {% endif %}
    <footer class="site-footer">
      {% block footer %}
        <div class="{{ container }}">
          <div class="site-footer__lg d-none d-lg-block">
            <div class="row">
              <div class="col-lg-5">
                {{ page.footer_first }}
              </div>
              <div class="col-lg-7">
                <div class="row">
                  <div class="col-md-3">
                    {{ page.footer_second }}
                  </div>
                  <div class="col-md-3">
                    {{ page.footer_third }}
                  </div>
                  <div class="col-md-3">
                    {{ page.footer_fourth }}
                  </div>
                  <div class="col-md-3">
                    {{ page.footer_fifth }}
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="site-footer__sm d-block d-lg-none">
            <div class="row">
              <div class="col-md-12">
                {{ page.footer_menu_sm }}
              </div>
            </div>
          </div>
          <div class="site-footer__bottom">
            {{ page.footer_bottom }}
          </div>
          <div class="site-footer__legal">
            {{ page.footer_legal }}
          </div>
        </div>
        {% if page.footer_simple %}
          <div class="site-footer__simple">
            {{ page.footer_simple }}
          {% endif %}
        </div>
      {% endblock %}
    </footer>
  </div>
</div>