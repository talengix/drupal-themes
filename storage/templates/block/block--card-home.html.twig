{#
/**
* @file
* Template for a block.
*/
#}
{% embed '@storage/block/block.twig' with {
  html_tag: 'div',
} %}
  {% block label %}
    <div{{ title_attributes.addClass(['block-title']) }}>{{ label }}</div>
  {% endblock %}

  {% block content %}
    <div{{ content_attributes.addClass('content') }}>
      <div class="card card-home">
        {{ content.field_image }}
        <div class="card-body">
          <h5 class="card-title">{{ content.field_title[0] }}</h5>
          <div class="card-text">{{ content.field_card_text[0] }}</div>
          <a href="/contact-us" class="btn btn-primary">learn more</a>
        </div>
      </div>
    </div>
  {% endblock %}
{% endembed %}